



div.action-bar {
	position:relative;
	height:36px;
  margin-top: 12px;
  border-top: 1px solid #f7f7f7;

	& div.actions {
		position:absolute;
		top:10px;
		height:36px;
		& button.button--material--flat.firstCardAction {
	    padding: 0 16px 0 0;
		}
	}
}


div.PDFDownload {
	padding:16px;
	text-align:center;

	& h1 {
		font-size:18px;
	}

 	& .fabContainer {
 		margin-bottom:10px;
		& .fab {
			margin:0 15px 15px 15px;
			& .fab__icon {
				line-height:56px;
			}
		}
	}
}
